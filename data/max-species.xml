<?xml version="1.0"?>
<ctml>
  <validate reactions="yes" species="yes"/>

  <!-- gas phase     -->
  <phase dim="3" id="gas">
    <elementArray datasrc="elements.xml">C H O N S Cl P Ar</elementArray>
    <speciesArray datasrc="nasa_gas.xml#species_data">all
      <skip element="undeclared"/>
    </speciesArray>
    <state>
      <temperature units="K">300.0</temperature>
      <pressure units="Pa">101325.0</pressure>
    </state>
    <thermo model="IdealGas"/>
    <kinetics model="GasKinetics"/>
    <transport model="None"/>
  </phase>

  <!-- condensed phase     -->
  <phase dim="3" id="solid">
    <elementArray datasrc="elements.xml">Si Ca Al Fe Na K Mg P Ti C H O N S Cl</elementArray>
    <speciesArray datasrc="nasa_condensed.xml#species_data">all
      <skip element="undeclared"/>
    </speciesArray>
    <state>
      <temperature units="K">300.0</temperature>
      <pressure units="Pa">101325.0</pressure>
    </state>
    <thermo model="IdealGas"/>
    <kinetics model="GasKinetics"/>
    <transport model="None"/>
  </phase>

  <!-- species definitions     -->
  <speciesData id="species_data"/>
  <reactionData id="reaction_data"/>
</ctml>
